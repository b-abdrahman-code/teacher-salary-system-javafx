<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="299.0" style="-fx-background-color: #ffff;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.max.BankAccountController">
   <children>
       <VBox alignment="CENTER" layoutX="6.0" spacing="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
           <padding>
               <Insets bottom="20" left="20" right="20" top="20" />
           </padding>
           <ListView fx:id="accountTypeListView" />
           <TextField fx:id="accountTypeTextField" promptText="Enter new account type" />
           <Button onAction="#handleAddAccountType" styleClass="login-btn" stylesheets="@../../../designFinal/design.css" text="Add Account Type">
            <font>
               <Font name="System Bold" size="13.0" />
            </font></Button>
           <Label fx:id="messageLabel" />
       </VBox>  </children>
</AnchorPane>
